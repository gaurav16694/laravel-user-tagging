{
    "name": "gaurav16694/laravel-user-tagging",
    "description": "End-to-end mentions in Laravel 5.",
    "keywords": [
        "laravel",
        "mentions"
    ],
    "homepage": "https://github.com/gaurav16694/laravel-user-tagging",
    "license": "MIT",
    "authors": [
        {
            "name": "gaurav kumar",
            "email": "gaurav16694@gmail.com",
            "homepage": "https://github.com/gaurav16694",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.4.0",
        "illuminate/database": "~5.4.0",
        "illuminate/http": "~5.4.0",
        "illuminate/routing": "~5.4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "gaurav\\tagging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gaurav\\tagging\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "gaurav\\tagging\\MentionServiceProvider"
            ]
        }
    }
}
